<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.chenhbzl.testffmpegjni"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.INTERNET" />
<!-- android:screenOrientation="landscape" portrait-->
    <application android:icon="@drawable/icon" android:label="@string/app_name"
    android:debuggable="true">
        <activity android:name="com.chenhbzl.testffmpegjni.TestFFmpegJNI"
                  android:label="@string/app_name"
                  android:screenOrientation="landscape"
                  android:theme="@style/Theme.Shelves">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity 
        		  android:name="com.chenhbzl.testffmpegjni.FFMpegFileExplorer"
                  android:label="@string/app_name"
                   android:theme="@style/Theme.Shelves">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>